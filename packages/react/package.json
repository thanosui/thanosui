{
  "name": "@thanosui/react",
  "version": "4.0.2",
  "license": "MIT",
  "homepage": "https://thanosui.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/thanosui/thanosui"
  },
  "keywords": [
    "react",
    "react ui",
    "components",
    "modern components",
    "react components",
    "tailwind"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint *.ts*",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "//de22v": "tsup --watch",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-mdx-gfm": "^7.4.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitest/coverage-c8": "^0.32.4",
    "eslint": "^8.48.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.29",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.0",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "framer-motion": "^10.16.4",
    "tailwind-variants": "^0.1.14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}